      "source": [
        "def chideuxadequation(p, X, alpha):\n",
        "    f_obs = [0 for i in p]\n",
        "    f_exp = len(X)*np.array(p)\n",
        "    for x in X:\n",
        "        f_obs[x] += 1\n",
        "    D,p_valeur=scipy.stats.chisquare(f_obs, f_exp)\n",
        "    nu=stats.chi2.ppf(1-alpha, len(p)-1)\n",
        "    return (D<nu)"
      ],
